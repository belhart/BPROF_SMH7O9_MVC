@model IEnumerable<string>
@{
    ViewData["Title"] = "Result of a weekend with engines";
}

<h1>Result of a weekend with engines</h1>

@if (Model.Count() == 0)
{
    <p>No data in the database</p>
}
else
{
<table>
    <tr>
        <th>The result of the weekend in order depending on the engine the team used</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item</td>
        </tr>
    }
</table>
}