@model IEnumerable<string>
@{
    ViewData["Title"] = "HomeEngineSumPointsList";
}

<h1>HomeEngineSumPointsList</h1>

@if (Model.Count() == 0)
{
    <p>No data in the database</p>
}
else
{
<table>
    <tr>
        <th>Versenyzo</th>
        <th>Elert pont</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item</td>
        </tr>
    }
</table>
}